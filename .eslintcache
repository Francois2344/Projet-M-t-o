,["58"],"import React, { useState } from \"react\";\nimport axios from \"axios\";\nimport WeatherSearch from \"./WeatherSearch\";\nimport Header from \"./Header\";\nimport Cards from \"./Cards\";\nimport WeatherData from \"./WeatherData\";\nimport Context from \"./Context\";\nimport Error from \"./Error\";\nimport Datetime from \"./Datetime\";\nimport Tagline from \"./Tagline\";\nimport Footer from \"./Footer\";\n\n\n\nconst Main = () => {\n  const [weather, setWeather] = useState()\n  const [city, setCity] = useState()\n  const [error, setError] = useState()\n  const [info, setInfo] = useState()\n\n  const api_call = async e => {\n    e.preventDefault()\n    const location = e.target.elements.location.value\n    if(!location) return setError(\"Merci d'entrer le nom d'une ville.\"), setWeather(null)\n    const API_KEY = \"3a42a2d988e037e1b4f125b8fc126b74\";\n    const url = `http://api.openweathermap.org/data/2.5/weather?q=${location}&units=metric&&appid=${API_KEY}&lang=fr`;\n    const request = axios.get(url);\n    const response = await request;\n    setWeather(response.data.main);\n    setCity(response.data.name);\n    setInfo(response.data.weather[0]);\n    setError(null);\n  };\n  console.log(info)\n\n  return (\n    <div className=\"weather\">\n      <Header />\n      <Cards>\n        <Tagline />\n        <Datetime />\n        <Context.Provider value = {{ api_call, weather, city, info, error }}>\n        <WeatherSearch />\n        {weather && info && <WeatherData />}\n      </Context.Provider>\n      {error && <Error error = {error} />}\n      </Cards>\n      <Footer />\n    </div>\n  );\n};\n\nexport default Main;\n","C:\\Users\\franc\\Desktop\\meteo-project\\src\\components\\WeatherSearch.jsx",[],"C:\\Users\\franc\\Desktop\\meteo-project\\src\\components\\WeatherData.jsx",[],"C:\\Users\\franc\\Desktop\\meteo-project\\src\\components\\Context.js",[],"C:\\Users\\franc\\Desktop\\meteo-project\\src\\components\\Error.jsx",[],"C:\\Users\\franc\\Desktop\\meteo-pro